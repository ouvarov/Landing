[ignore]
.*/__tests__/.*
<PROJECT_ROOT>/\(node_modules\|build\|stories\)/.*

[include]

[libs]
flow-typed

[lints]

[options]
suppress_comment= \\(.\\|\n\\)*\\$FlowIgnore
module.file_ext = .js
module.file_ext = .jsx
module.file_ext = .mjs
module.file_ext = .json
module.file_ext = .sass
module.file_ext = .scss

[strict]
unclear-type
